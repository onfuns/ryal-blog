{
  "name": "nest-blog",
  "version": "2.0.0",
  "private": "true",
  "description": "blog developed by nestjs",
  "scripts": {
    "dev": "lerna run dev",
    "dev:admin": "lerna run dev --scope @nest-blog/admin",
    "dev:client": "lerna run dev --scope @nest-blog/client",
    "dev:server": "lerna run dev --scope @nest-blog/server",
    "build": "lerna run build",
    "build:admin": "lerna run build --scope @nest-blog/admin",
    "build:client": "lerna run build --scope @nest-blog/client",
    "build:server": "lerna run build --scope @nest-blog/server",
    "prepare": "npx husky install",
    "bootstrap": "lerna bootstrap"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onfuns/nestjs-blog.git"
  },
  "homepage": "https://github.com/onfuns/nestjs-blog#readme",
  "author": "onfuns",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@unocss/preset-rem-to-px": "^0.59.4",
    "@unocss/preset-wind": "^0.59.4",
    "@unocss/reset": "^0.59.4",
    "cross-env": "^7.0.3",
    "eslint": "^9.2.0",
    "eslint-plugin-react": "^7.34.1",
    "fs-extra": "^10.0.0",
    "husky": "^7.0.0",
    "lerna": "^6.6.2",
    "lint-staged": "^12.1.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "stylelint": "^16.6.1",
    "unocss": "^0.59.4"
  },
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.0"
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  }
}
